{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "templateBaseUrl": {
      "type": "string",
      "defaultValue": "https://raw.githubusercontent.com/elastic/azure-marketplace/hackfest/src/",
      "metadata": {
        "description": "Base uri of resources"
      }
    },
    "adminUsername": {
      "type": "string",
      "defaultValue": ""
    },
    "password": {
      "type": "object",
      "defaultValue": ""
    },
    "storageAccountName": {
      "type": "string",
      "defaultValue": ""
    },
    "location": {
      "type": "string",
      "defaultValue": ""
    },
    "subnet": {
      "type": "object",
      "defaultValue": {}
    },
    "masterNodesIpPrefix": {
      "type": "string",
      "defaultValue": ""
    },
    "vmSize": {
      "type": "string",
      "defaultValue": ""
    },
    "networkResourceGroup": {
      "type": "string",
      "metadata": {
        "description": "The resource group in which the network resides"
      }
    },
    "osSettings": {
      "type": "object",
      "defaultValue": {}
    }
  },
  "variables": {},
  "resources": [],
  "outputs": {}
}
